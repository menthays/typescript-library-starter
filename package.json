{
  "name": "typescript-library-starter",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Scaffold for writing library in typescript with coooool tools.",
  "repository": {
    "type": "git",
    "url": "https://github.com/menthays/typescript-library-starter.git"
  },
  "scripts": {
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "test": "jest",
    "build": "pack build",
    "publish": "pack publish",
    "version": "npm run build"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-typescript": "7.6.0",
    "@pika/pack": "0.4.0",
    "@pika/plugin-build-node": "0.6.0",
    "@pika/plugin-build-types": "0.6.0",
    "@pika/plugin-build-web": "0.6.0",
    "@pika/plugin-standard-pkg": "0.6.0",
    "@types/jest": "24.0.18",
    "docz": "1.2.0",
    "docz-theme-default": "1.2.0",
    "jest": "24.9.0",
    "typescript": "3.6.2"
  }
}
